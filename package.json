{
  "name": "@nordicsemiconductor/asset-tracker-cloud-memfault-aws",
  "version": "0.0.0-development",
  "description": "Memfault integration for AWS IoT developed using AWS CDK in TypeScript.",
  "type": "module",
  "scripts": {
    "prepare": "husky install && check-node-version --package && npm run compile",
    "compile": "swc ./cdk -d dist/cdk && swc ./feature-runner -d dist/feature-runner",
    "test": "jest",
    "test:e2e": "node --unhandled-rejections=strict dist/feature-runner/run-features.js ./features --print-results --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/asset-tracker-cloud-memfault-aws-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/asset-tracker-cloud-memfault-aws-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/asset-tracker-cloud-memfault-aws-js#readme",
  "keywords": [
    "nrf-asset-tracker",
    "iot",
    "memfault",
    "aws",
    "cdk"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "3.296.0",
    "@aws-sdk/client-dynamodb": "3.296.0",
    "@aws-sdk/client-iot": "3.296.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.8",
    "@nordicsemiconductor/cloudformation-helpers": "7.0.7",
    "@nordicsemiconductor/e2e-bdd-test-runner": "16.0.14",
    "@nordicsemiconductor/object-to-env": "5.0.0",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.41",
    "@swc/jest": "0.2.24",
    "@types/aws-iot-device-sdk": "2.2.4",
    "@types/aws-lambda": "8.10.114",
    "@types/backoff": "2.5.2",
    "@types/chai-subset": "1.3.3",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.5",
    "@types/yazl": "2.4.2",
    "aws-cdk-lib": "2.69.0",
    "aws-iot-device-sdk": "2.2.12",
    "backoff": "2.5.0",
    "cdk": "2.69.0",
    "chai": "4.3.7",
    "chai-subset": "1.6.0",
    "chalk": "5.2.0",
    "check-node-version": "4.2.1",
    "chokidar": "3.5.3",
    "constructs": "10.1.285",
    "dependency-tree": "9.0.0",
    "esbuild": "0.17.12",
    "glob": "8.1.0",
    "jest": "29.5.0",
    "regenerator-runtime": "0.13.11",
    "typescript": "5.0.2",
    "yazl": "2.5.1"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "dependencies": {
    "@aws-sdk/client-ssm": "3.296.0"
  },
  "jest": {
    "transform": {
      ".+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testRegex": ".+\\.spec\\.ts$"
  }
}
